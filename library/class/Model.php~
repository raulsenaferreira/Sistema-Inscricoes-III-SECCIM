<?php
abstract class Model {
 
	protected $id;
	
	public function __construct($id = NULL){
		if($id != NULL){
			$this->carregarPorId($id);
		}
	}
	
	#-------------------------------------#
	# GET
	#-------------------------------------#
	
	
	
	#-------------------------------------#
	# METHODS
	#-------------------------------------#
	
	
	
	
	
	
	
 	  	
}// end class

?>